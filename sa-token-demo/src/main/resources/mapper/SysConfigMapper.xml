<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hjc.demo.mapper.SysConfigMapper">

    <resultMap id="BaseResultMap" type="com.hjc.demo.domain.SysConfig">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="configName" column="config_name" jdbcType="VARCHAR"/>
            <result property="configKey" column="config_key" jdbcType="VARCHAR"/>
            <result property="configValue" column="config_value" jdbcType="VARCHAR"/>
            <result property="configType" column="config_type" jdbcType="CHAR"/>
            <result property="configGroup" column="config_group" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,config_name,config_key,
        config_value,config_type,config_group
    </sql>
</mapper>
