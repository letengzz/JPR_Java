<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hjc.demo.mapper.SysDictTypeMapper">

    <resultMap id="BaseResultMap" type="com.hjc.demo.domain.SysDictType">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="dictName" column="dict_name" jdbcType="VARCHAR"/>
            <result property="dictType" column="dict_type" jdbcType="VARCHAR"/>
            <result property="dictStatus" column="dict_status" jdbcType="CHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,dict_name,dict_type,
        dict_status
    </sql>
</mapper>
